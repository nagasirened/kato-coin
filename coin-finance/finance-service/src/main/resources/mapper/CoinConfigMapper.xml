<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.naga.mapper.CoinConfigMapper">

    <resultMap id="BaseResultMap" type="com.naga.domain.CoinConfig">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="coinType" column="coin_type" jdbcType="VARCHAR"/>
            <result property="creditLimit" column="credit_limit" jdbcType="DECIMAL"/>
            <result property="creditMaxLimit" column="credit_max_limit" jdbcType="DECIMAL"/>
